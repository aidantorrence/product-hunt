generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("CONNECTION_URL")
}

model codesnippets {
  id         String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  v          Int      @map("__v")
  category   String
  createdAt  DateTime @db.Date
  difficulty String
  language   String
  prompt     String
  solution   String
  title      String
}
